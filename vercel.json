{
    "version": 2,
    "rewrites": [
        { "source": "/(.*)", "destination": "api/index.js" }
      ],
      "functions": {
        "api/index.js": {
          "maxDuration": 60
        }
      },
    

    "crons": [
        {
          "path": "/getData",
          "schedule": "15 15 * * *"
        },
        {
          "path" : "/getRefreshToken",
          "schedule": "0 15 * * *"
        }
      ]

   
   }