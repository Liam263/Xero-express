{
    "version": 2,
    "rewrites": [
        { "source": "/(.*)", "destination": "index.js" }
      ],
      "functions": {
        "server.js": {
          "maxDuration": 60
        }
      },
    

    "routes": [
     {
      "src": "/(.*)",
      "dest": "index.js"
     }
    ],

    "crons": [
        {
          "path": "/getData",
          "schedule": "5 0 * * *"
        },
        {
          "path" : "/getRefreshToken",
          "schedule": "5 0 * * *"
        }
      ]

   
   }