{
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] },
      "functions": {
            "/*.js": {
                "maxDuration": 60
            }
        
      }
     }
    ],

    "routes": [
     {
      "src": "/(.*)",
      "dest": "index.js"
     }
    ],

    "crons": [
        {
          "path": "/getData",
          "schedule": "5 0 * * *"
        },
        {
          "path" : "/getRefreshToken",
          "schedule": "5 0 * * *"
        }
      ]

   
   }