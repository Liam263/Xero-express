{
    "version": 2,
    "builds": [
     {
      "src": "index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "index.js"
     }
    ],
    "crons": [
        {
          "path": "/getData",
          "schedule": "15 23 * * *"
        },
        {
            "path" : "/getRefreshToken",
            "schedule": "15 23 * * *"
        }
      ]
   }