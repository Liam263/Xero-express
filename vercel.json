{
    "version": 2,
    
    "functions": {
        "index.js": {
            "maxDuration": 60
        }
    },

    "routes": [
     {
      "src": "/(.*)",
      "dest": "index.js"
     }
    ],

    "crons": [
        {
          "path": "/getData",
          "schedule": "5 0 * * *"
        },
        {
          "path" : "/getRefreshToken",
          "schedule": "5 0 * * *"
        }
      ]

   
   }